/* Sticky footer styles from http://getbootstrap.com/examples/sticky-footer-navbar/*/
html
  position: relative
  min-height: 100%

body
  /* Margin bottom by footer height */
  margin-bottom: 60px

.footer
  position: absolute
  bottom: 0
  width: 100%
  /* Set the fixed height of the footer here */
  height: 60px
  background-color: #0c2e28

.custom-footer-logo
  padding-top: 0px
  width: 30px
  height: 30px
