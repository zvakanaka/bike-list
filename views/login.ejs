<!DOCTYPE html>
<html>
  <head>
    <title>Login</title>
    <% if (process.env.SUB_APP && process.env.OUTER_APP_DIR) {%>
      <%- include(process.env.OUTER_APP_DIR+'/views/partials/head') %>
    <% } else { %>
      <% include partials/head %>
    <% } %>
  </head>
  <body>
    <% if (process.env.SUB_APP && process.env.OUTER_APP_DIR) {%>
      <%- include(process.env.OUTER_APP_DIR+'/views/partials/nav') %>
    <% } else { %>
      <% include partials/nav %>
    <% } %>

    <div class="container-fluid">
      <div id="login" class="panel panel-default">
        <div class="panel-heading">
          <h3 class="panel-title">Log in</h3>
        </div>
          <div class="panel-body">
            <a href="/auth/google">Log In!</a>
          </div>
        </div>
        <% if (process.env.SUB_APP && process.env.OUTER_APP_DIR) {%>
          <%- include(process.env.OUTER_APP_DIR+'/views/partials/footer') %>
        <% } else { %>
          <% include partials/footer %>
        <% } %>
      </body>
    </html>
